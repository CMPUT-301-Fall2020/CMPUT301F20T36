<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.book_master">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".add_book_activity"></activity>
        <activity android:name=".search_page_activity" />
        <activity android:name=".owner_receive_activity" />
        <activity android:name=".borrow_return_activity" />
        <activity android:name=".return_activity" />
        <activity android:name=".borrow_list_activity" />
        <activity android:name=".check_list_activity" />
        <activity android:name=".main_menu_activity" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>