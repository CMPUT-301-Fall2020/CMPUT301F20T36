<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/image1">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="180dp"
        android:layout_height="38dp"
        android:layout_marginStart="124dp"
        android:layout_marginLeft="124dp"
        android:layout_marginTop="64dp"
        android:text="BookMaster"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="143dp"
        android:layout_height="28dp"
        android:layout_marginStart="140dp"
        android:layout_marginLeft="140dp"
        android:layout_marginTop="20dp"
        android:text="Hello, CMPUT301"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="267dp"
        android:layout_height="26dp"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        android:layout_marginTop="16dp"
        android:text="What do you want to do today?"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/Logout_main_button"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="24dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Log Out"
        app:layout_constraintStart_toEndOf="@+id/main_menu_request"
        app:layout_constraintTop_toBottomOf="@+id/main_menu_borrower_show_requested" />

    <Button
        android:id="@+id/Return_main_button"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="40dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Return"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Borrow_main_button" />

    <Button
        android:id="@+id/Receive_main_button"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="40dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Recieve"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CHECKMYLIST_button" />

    <Button
        android:id="@+id/Borrow_main_button"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="40dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Borrow"
        app:layout_constraintStart_toEndOf="@+id/Receive_main_button"
        app:layout_constraintTop_toBottomOf="@+id/Search_main_button" />

    <Button
        android:id="@+id/CHECKMYLIST_button"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="40dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Checkmylist"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_profile_button" />

    <Button
        android:id="@+id/Search_main_button"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="40dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Search Book"
        app:layout_constraintStart_toEndOf="@+id/CHECKMYLIST_button"
        app:layout_constraintTop_toBottomOf="@+id/main_menu_retrieve_profile" />

    <Button
        android:id="@+id/view_profile_button"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="40dp"
        android:background="#80D4D5D6"
        android:text="View Profile"
        android:textColor="#FF000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/main_menu_retrieve_profile"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="40dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Search Profile"
        app:layout_constraintStart_toEndOf="@+id/view_profile_button"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/main_menu_request"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="24dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Request"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Return_main_button" />

    <Button
        android:id="@+id/main_menu_borrower_show_requested"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="40dp"
        android:textColor="#FF000000"
        android:background="#80D4D5D6"
        android:text="Show Requested"
        app:layout_constraintStart_toEndOf="@+id/Return_main_button"
        app:layout_constraintTop_toBottomOf="@+id/Borrow_main_button" />

</androidx.constraintlayout.widget.ConstraintLayout>